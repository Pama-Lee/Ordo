{
  "name": "ordo-editor",
  "private": true,
  "version": "0.1.0",
  "description": "Ordo Rule Engine Visual Editor Components",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @ordo-engine/playground dev",
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @ordo-engine/editor-core build",
    "build:vue": "pnpm --filter @ordo-engine/editor-vue build",
    "build:react": "pnpm --filter @ordo-engine/editor-react build",
    "build:packages": "pnpm build:core && pnpm build:vue && pnpm build:react",
    "test": "pnpm -r test",
    "lint": "eslint . --ext .ts,.tsx,.vue",
    "lint:fix": "eslint . --ext .ts,.tsx,.vue --fix",
    "format": "prettier --write \"**/*.{ts,tsx,vue,json,md}\"",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
