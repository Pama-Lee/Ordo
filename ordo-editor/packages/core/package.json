{
  "name": "@ordo-engine/editor-core",
  "version": "0.1.0",
  "description": "Core logic for Ordo Rule Editor (framework-agnostic)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ordo-engine/wasm": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "ordo",
    "rule-engine",
    "editor",
    "core"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pama-Lee/Ordo.git",
    "directory": "ordo-editor/packages/core"
  },
  "publishConfig": {
    "access": "public"
  }
}
