{
  "name": "@ordo-engine/editor-vue",
  "version": "0.2.0",
  "description": "Vue 3 components for Ordo Rule Editor",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ordo-engine/editor-core": "workspace:*",
    "@types/dagre": "^0.7.53",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.48.1",
    "@vue-flow/minimap": "^1.5.4",
    "@vue-flow/node-resizer": "^1.5.0",
    "dagre": "^0.8.5"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "ordo",
    "rule-engine",
    "editor",
    "vue",
    "vue3"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pama-Lee/Ordo.git",
    "directory": "ordo-editor/packages/vue"
  },
  "publishConfig": {
    "access": "public"
  }
}
