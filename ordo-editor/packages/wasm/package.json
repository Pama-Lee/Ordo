{
  "name": "@ordo/wasm",
  "version": "0.1.0",
  "description": "WebAssembly bindings for Ordo Rule Engine",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts",
    "stub.js",
    "stub.d.ts"
  ],
  "scripts": {
    "build": "cd ../../crates/ordo-wasm && ./build.sh",
    "build:stub": "echo 'Using stub module'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ordo",
    "rule-engine",
    "wasm",
    "webassembly"
  ],
  "license": "MIT"
}

